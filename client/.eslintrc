{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "semi": [0],
    "react/jsx-filename-extension": [0],
    "react/jsx-one-expression-per-line": [0],
    "comma-dangle": [0],
    "import/no-unresolved": [0],
    "react/require-default-props": [0],
    "react/prefer-stateless-function": [0],
    "react/destructuring-assignment": [0]
  }
}
